        </div>
        <div class="span8">
            <div id="profile-container" >
            </div>
        </div>
        <div class="span2">
        </div>
    </div>
    <div class="row-fluid">
        <div class="span2">
        </div>
        <div class="span8">
            <div class="row-fluid">
                <div class="span5">
                    <div class="well sidebar-nav">
                        <div class="dashboard">  
                            <div data-component-term="mini_home_profile" class="module mini-profile">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="content-main span7">
                    <div class="content-header">
                        <div class="header-inner">
                            <h2>
                                <span class="js-stream-title"></span>
                            </h2>
                        </div>
                        <div class='js-search-form content-searchbar hide'>
                            <table class="flex-table">
                                <tbody>
                                    <tr>
                                        <td class="flex-table-primary">
                                            <div class="flex-table-form">
                                                <input type="text" placeholder="Search using a person’s full name or @username" class="flex-table-input search-query">
